<VirtualHost *:80>

	DocumentRoot /var/www/html/
	
        RewriteEngine On
        RewriteRule /.well-known/carddav /card.php [R,L]
        RewriteRule /.well-known/caldav /cal.php [R,L]

	<Directory "/var/www/html">
		Options None
		Options +FollowSymlinks
		AllowOverride All
	</Directory>

</VirtualHost>
